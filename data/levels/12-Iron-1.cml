  <molecule title="Fe2O3(s)"/>
  <molecule title="Fe2O3(s)"/>
  <molecule title="Fe2O3(s)"/>
    <molecule title="O2(g)"/>
    <molecule title="O2(g)"/>
    <molecule title="O2(g)"/>
    <molecule title="O2(g)"/>
  </moleculeList>
  <effectList>
	  <effect title="Mining" x2="200" y2="550">
	    <molecule title="Fe2O3(s)"/>
	    <molecule title="Fe3O4(s)"/>
	  </effect>
    <effect title="Fire" value="2300" x2="800" y2="270"/>
