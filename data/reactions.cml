      <molecule title="HCl"/>
      <molecule title="HCl"/>
      <molecule title="HCl"/>
    </reactantList>
    <productList>
      <molecule title="C6H6BrN(s)"/>
      <molecule title="H2O(l)"/>
      <molecule title="H2O(l)"/>
      <molecule title="ZnCl2(s)"/>
      <molecule title="ZnCl2(s)"/>
      <molecule title="ZnCl2(s)"/>
